interface Pessoa {
    funcao falar();
}

interface Funcionario {
    funcao trabalhar();
}

classe Worker implementa Funcionario, Pessoa {
    funcao trabalhar() {
      escrever("Amo trabalhar...");
    }

    funcao falar() {
      escrever("Oi");
    }
}

funcao t(a: Inteiro, b: Inteiro) {
  dormir(500);
  var w = novo Worker();
  w.trabalhar();
  retornar a + b;
}

var resposta = executar(t, 4, 3);
escrever(resposta);


